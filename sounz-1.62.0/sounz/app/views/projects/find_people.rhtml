
<% if @results.length > 0 %>
<ul>
<% for person in @results 
person_id = generate_id(person)
%>
<li id="<%=generate_id(person)%>"><%= link_to_remote( "Add", :complete => "new Effect.Highlight('"+person_id+"')",
:url => { :action => 'add_person', :project_id => @project.project_id, :person_id => person.person_id } ) -%> <%=h person.full_name %>:</li>
<% end %>
</ul>
<% else %>
Sorry, no results
<% end %>

