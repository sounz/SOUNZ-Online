<h1>SOUNZtender Bidding Form</h1>

<br/>

<p>
  <b>Name:</b>
  <%=h @prov_bid.name %>
</p>

<p>
  <b>Email:</b>
  <%=h @prov_bid.email %>
</p>

<p>
  <b>Phone home:</b>
  <%=h @prov_bid.phone_home %>
</p>

<p>
  <b>Phone work:</b>
  <%=h @prov_bid.phone_work %>
</p>

<p>
  <b>Phone mobile:</b>
  <%=h @prov_bid.phone_mobile %>
</p>

<p>
  <b>Phone fax:</b>
  <%=h @prov_bid.phone_fax %>
</p>

<p>
  <b>Postal address:</b>
  <%=h @prov_bid.postal_address %>
</p>

<p>
  <b>Postcode:</b>
  <%=h @prov_bid.postcode %>
</p>

<p>
  <b>Is group bid:</b>
  <%=h @prov_bid.is_group_bid %>
</p>

<p>
  <b>Group name:</b>
  <%=h @prov_bid.group_name %>
