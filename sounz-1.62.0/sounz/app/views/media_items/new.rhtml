<h1>New media_item</h1>

<%= error_messages_for :media_item %>

<% form_for(:media_item, :url => media_items_path, :html => { :multipart => true }) do |form| %>
  <p>
    <label for="uploaded_data">Upload a file:</label>
    <%= form.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', media_items_path %>