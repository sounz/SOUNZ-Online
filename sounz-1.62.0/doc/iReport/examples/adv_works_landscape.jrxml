	<queryString><![CDATA[SELECT
     work_composers."work_title" AS work_composers_work_title,
     work_composers."work_subcategory_desc" AS work_composers_work_subcategory_desc,
     works."intended_duration" AS works_intended_duration,
     works."difficulty" AS works_difficulty,
     contributors."known_as" AS contributors_known_as,
     manifestations."item_cost" AS manifestations_item_cost
FROM
     "public"."work_composers" work_composers INNER JOIN "public"."works" works ON work_composers."work_id" = works."work_id"
     INNER JOIN "public"."people" people ON work_composers."person_id" = people."person_id"
     INNER JOIN "public"."contributors" contributors ON work_composers."contributor_id" = contributors."contributor_id"
     INNER JOIN "public"."expressions" expressions ON works."work_id" = expressions."work_id"
     INNER JOIN "public"."expression_manifestations" expression_manifestations ON expressions."expression_id" = expression_manifestations."expression_id"
     INNER JOIN "public"."manifestations" manifestations ON expression_manifestations."manifestation_id" = manifestations."manifestation_id"
WHERE
     works."difficulty" = 3
 AND work_composers."work_subcategory_desc" LIKE $P{SubcatDesc}]]></queryString>

