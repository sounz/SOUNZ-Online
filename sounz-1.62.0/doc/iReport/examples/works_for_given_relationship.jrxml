	<queryString><![CDATA[SELECT
     works."work_title" AS works_work_title,
     works."difficulty" AS works_difficulty,
     people."known_as" AS contributor_name,
     'P'::text as thetype,
     role_types."role_type_desc" AS role_types_role_type_desc,
     work_subcategories."work_subcategory_desc" AS work_subcategories_work_subcategory_desc,
     works."intended_duration" AS works_intended_duration,
     works."work_description" AS works_work_description,
     works."applicable_for_youth" AS works_applicable_for_youth,
     relationship_types."relationship_type_desc" AS relationship_types_relationship_type_desc
FROM
     "public"."work_subcategories" work_subcategories INNER JOIN "public"."works" works ON work_subcategories."work_subcategory_id" = works."work_subcategory_id"
     INNER JOIN "public"."work_categories" work_categories ON work_subcategories."work_category_id" = work_categories."work_category_id"
     INNER JOIN "public"."work_relationships" work_relationships ON works."work_id" = work_relationships."work_id"
     INNER JOIN "public"."relationships" relationships ON work_relationships."relationship_id" = relationships."relationship_id"
     INNER JOIN "public"."relationship_types" relationship_types ON work_relationships."relationship_type_id" = relationship_types."relationship_type_id"
     INNER JOIN "public"."role_relationships" role_relationships ON relationships."relationship_id" = role_relationships."relationship_id"
     INNER JOIN "public"."roles" roles ON role_relationships."role_id" = roles."role_id"
     INNER JOIN "public"."role_types" role_types ON roles."role_type_id" = role_types."role_type_id"
     INNER JOIN "public"."people" people ON roles."person_id" = people."person_id"
     WHERE relationship_types.relationship_type_desc ILIKE $P{RelType}
UNION
SELECT
     works."work_title" AS works_work_title,
     works."difficulty" AS works_difficulty,
     organisations."organisation_abbrev" AS contributor_name,
     'O'::text as thetype,
     role_types."role_type_desc" AS role_types_role_type_desc,
     work_subcategories."work_subcategory_desc" AS work_subcategories_work_subcategory_desc,
     works."intended_duration" AS works_intended_duration,
     works."work_description" AS works_work_description,
     works."applicable_for_youth" AS works_applicable_for_youth,
     relationship_types."relationship_type_desc" AS relationship_types_relationship_type_desc
FROM
     "public"."work_subcategories" work_subcategories INNER JOIN "public"."works" works ON work_subcategories."work_subcategory_id" = works."work_subcategory_id"
     INNER JOIN "public"."work_categories" work_categories ON work_subcategories."work_category_id" = work_categories."work_category_id"
     INNER JOIN "public"."work_relationships" work_relationships ON works."work_id" = work_relationships."work_id"
     INNER JOIN "public"."relationships" relationships ON work_relationships."relationship_id" = relationships."relationship_id"
     INNER JOIN "public"."relationship_types" relationship_types ON work_relationships."relationship_type_id" = relationship_types."relationship_type_id"
     INNER JOIN "public"."role_relationships" role_relationships ON relationships."relationship_id" = role_relationships."relationship_id"
     INNER JOIN "public"."roles" roles ON role_relationships."role_id" = roles."role_id"
     INNER JOIN "public"."role_types" role_types ON roles."role_type_id" = role_types."role_type_id"
     INNER JOIN "public"."organisations" organisations ON roles."organisation_id" = organisations."organisation_id"
     WHERE relationship_types.relationship_type_desc ILIKE $P{RelType}]]></queryString>

