--------------------------------------------------------------------
-- Set up SOUNZ database access privileges
-- Basically we have three users who can access the database:
--
--  sounz_admin     ALL privileges on all objects
--  sounz_web       Sel, Ins, Upd, Del privileges on all tables
--  sounz_reports   Sel privilege on all tables only
--------------------------------------------------------------------

-- No public access
REVOKE ALL PRIVILEGES FROM PUBLIC;

-- The Postgresql users - will change passwords when going live
CREATE USER SOUNZ_ADMIN   WITH LOGIN ENCRYPTED PASSWORD 'excalibur';
CREATE USER SOUNZ_WEB     WITH LOGIN ENCRYPTED PASSWORD 'rails';
CREATE USER SOUNZ_REPORTS WITH LOGIN ENCRYPTED PASSWORD 'sounz';

-- All table permissions
GRANT SELECT ON access_rights TO SOUNZ_REPORTS;
GRANT ALL ON access_rights TO SOUNZ_ADMIN;
GRANT SELECT,INSERT,DELETE,UPDATE ON access_rights TO SOUNZ_WEB;

GRANT SELECT ON allowed_ips TO SOUNZ_REPORTS;
GRANT ALL ON allowed_ips TO SOUNZ_ADMIN;
GRANT SELECT,INSERT,DELETE,UPDATE ON allowed_ips TO SOUNZ_WEB;

